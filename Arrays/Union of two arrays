class Solution{
    public static int doUnion(int a[], int n, int b[], int m) 
    {
        HashSet<Integer> anss = new HashSet<Integer>();
        for(int i=0;i<n;i++)
        {
            anss.add(a[i]);
        }
        for(int i=0;i<m;i++)
        {  
            anss.add(b[i]);
        }
        return anss.size();
    }
}
